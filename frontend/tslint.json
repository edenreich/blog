{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "always"],
        "no-console": false,
        "quotemark": [true, "single", "jsx-double"],
        "eofline": true
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    }
}