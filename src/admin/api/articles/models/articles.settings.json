{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "articles"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "uuid": {
      "type": "uid",
      "private": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "published_at": {
      "type": "datetime"
    },
    "published": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "likes": {
      "collection": "likes",
      "via": "article"
    }
  }
}
