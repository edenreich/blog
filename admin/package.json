{
  "name": "admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r module-alias/register ./src/app.js",
    "build": "tsc && yarn run build:assets && cp -r src/static build/src/static && cp package* build/ && cd build && yarn install --production",
    "build:assets": "encore production --progress",
    "dev": "nodemon --watch 'src/**' --ext 'ts,twig' --exec 'encore dev && ts-node -r module-alias/register src/app.ts'",
    "lint": "tslint './src/**/*.ts' './__tests__/**/*.ts'",
    "lint:fix": "tslint --fix './src/**/*.ts' './__tests__/**/*.ts'"
  },
  "keywords": [
    "admin"
  ],
  "author": "Eden Reich",
  "license": "MIT",
  "dependencies": {
    "admin-lte": "^3.1.0",
    "axios": "^0.21.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.1",
    "module-alias": "^2.2.2",
    "twig": "^1.15.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.13.13",
    "@symfony/webpack-encore": "^1.3.0",
    "@types/axios": "^0.14.0",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.2",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^7.0.0",
    "@types/twig": "^1.12.5",
    "nodemon": "^2.0.7",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "ts-lint": "^4.5.1",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-notifier": "^1.13.0"
  },
  "_moduleAliases": {
    "@app": "./src"
  }
}
