    "@types/koa-router": "^7.4.2",
    "@types/twig": "^1.12.5",
    "nodemon": "^2.0.7",
    "ts-lint": "^4.5.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "module-alias": "^2.2.2",
